@inherits LayoutComponentBase

<Layout>
    <LayoutHeader>
        <Bar Breakpoint="Breakpoint.Desktop" Background="Background.Dark" ThemeContrast="ThemeContrast.Dark">
            <BarBrand>
                <BarItem>
                    <BarLink To="/">
                        <QuibbleIcon Size="32" />
                    </BarLink>
                </BarItem>
            </BarBrand>
            <BarToggler />
            <BarMenu>
                <BarEnd>
                    <LoginDisplay />
                </BarEnd>
            </BarMenu>
        </Bar>
    </LayoutHeader>
    <LayoutContent Padding="Padding.Is5.OnY">
        @Body
    </LayoutContent>
    <LayoutFooter>
    </LayoutFooter>
</Layout>

